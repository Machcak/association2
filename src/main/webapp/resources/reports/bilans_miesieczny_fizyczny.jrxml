	<parameter isForPrompting="true" name="datefrom" class="java.util.Date"/>
	<parameter isForPrompting="true" name="dateto" class="java.util.Date"/>
	
	<queryString language="SQL">
		<![CDATA[SELECT o.*,  ROUND((@amounttotal := amount + @amounttotal),2) as amounttotal
FROM(
			SELECT b.id,  incomingdate, eventname, if(eventname='Składka' or eventname='Bilans otwarcia' or eventname='Kara za zwłokę w opłaceniu składki', t.termname, null) as termname, lastname, firstname,  if(incometo_id = 4 , (amount * -1)  , amount ) as amount, incometo_id, it.priority, it.title
			FROM balance b 
			JOIN balanceevent e on b.balanceevent_id = e.id 
			LEFT JOIN associationmember m on m.id = b.member_id 
			JOIN balanceterm t on t.id = b.balanceterm_id 
			JOIN incometo it on it.id = b.incometo_id 
			WHERE incomingdate between $P{datefrom} and DATE_FORMAT($P{dateto},'%Y-%m-%d')
			AND  ( it.title = 'Kasa'  or it.title = 'Operacja na bankomacie'  or  it.title = 'Bilans Otwarcia Miesiąca')
		  ) o,
  (SELECT @amounttotal := ( SELECT COALESCE(SUM(if(incometo_id = 4, amount * -1, amount)), 0) 
												  FROM balance b 
												  JOIN incometo it on it.id = b.incometo_id 
												  WHERE incomingdate < DATE_FORMAT($P{datefrom},'%Y-%m-%d')
												  AND  (it.title = 'Kasa'  or it.title = 'Operacja na bankomacie'  or  it.title = 'Bilans Otwarcia Miesiąca')
												  ) 
  ) c
ORDER BY incomingdate, priority desc, amount desc]]>
